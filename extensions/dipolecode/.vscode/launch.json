{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run in dipoleSTUDIO",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "C:/repos/dipoleSTUDIO/scripts/code.bat",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "npm: compile"
    },
    {
      "name": "Run in dipoleSTUDIO (No Build)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "C:/repos/dipoleSTUDIO/scripts/code.bat",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
    {
      "name": "Run Extension (VS Code)",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--disable-extensions",
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "npm: compile"
    }
  ]
}
