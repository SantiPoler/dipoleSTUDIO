{
  "name": "dipolecode",
  "displayName": "dipoleCODE",
  "description": "dipoleCODE - AI Coding Assistant by dipoleSTUDIO",
  "version": "1.1.20",
  "publisher": "sst-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/anomalyco/opencode"
  },
  "license": "MIT",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dipolecode.openTerminal",
        "title": "Open dipoleCODE",
        "icon": {
          "light": "images/button-dark.svg",
          "dark": "images/button-light.svg"
        }
      },
      {
        "command": "dipolecode.openNewTerminal",
        "title": "Open dipoleCODE in new tab",
        "icon": {
          "light": "images/button-dark.svg",
          "dark": "images/button-light.svg"
        }
      },
      {
        "command": "dipolecode.addFilepathToTerminal",
        "title": "Add Filepath to Terminal"
      },
      {
        "command": "dipolecode.checkAFWK",
        "title": "dipoleCODE: Configurar AFWK"
      },
      {
        "command": "dipolecode.createAFWK",
        "title": "dipoleCODE: Crear estructura AFWK"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dipolecode-activitybar",
          "title": "dipoleCODE",
          "icon": "images/activity-icon.svg"
        }
      ]
    },
    "views": {
      "dipolecode-activitybar": [
        {
          "id": "dipolecode.sidebar",
          "name": "dipoleCODE",
          "visibility": "hidden"
        }
      ]
    },
    "menus": {
      "editor/title": [
        {
          "command": "dipolecode.openNewTerminal",
          "group": "navigation"
        }
      ]
    },
"configuration": {
      "title": "dipoleCODE",
      "properties": {
        "dipolecode.afwk.preference": {
          "type": "string",
          "enum": ["full", "none", "pending"],
          "default": "pending",
          "description": "Preferencia de integraci贸n AFWK para este proyecto",
          "enumDescriptions": [
            "Integraci贸n AFWK completa habilitada",
            "Usar dipoleCODE sin integraci贸n AFWK",
            "Pendiente de decisi贸n del usuario"
          ]
        }
      }
    },
    "keybindings": [
      {
        "command": "dipolecode.openTerminal",
        "title": "Run dipoleCODE",
        "key": "cmd+escape",
        "mac": "cmd+escape",
        "win": "ctrl+escape",
        "linux": "ctrl+escape"
      },
      {
        "command": "dipolecode.openNewTerminal",
        "title": "Run dipoleCODE",
        "key": "cmd+shift+escape",
        "mac": "cmd+shift+escape",
        "win": "ctrl+shift+escape",
        "linux": "ctrl+shift+escape"
      },
      {
        "command": "dipolecode.addFilepathToTerminal",
        "title": "dipoleCODE: Insert At-Mentioned",
        "key": "cmd+alt+k",
        "mac": "cmd+alt+k",
        "win": "ctrl+alt+K",
        "linux": "ctrl+alt+K"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "bun run package",
    "compile": "bun run check-types && bun run lint && node esbuild.js",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "bun run check-types && bun run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "bun run compile-tests && bun run compile && bun run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "esbuild": "^0.25.3",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
